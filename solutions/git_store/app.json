{
  "name": "Bitwarden_rs-git_store",
  "description": "Bitwardenrs on Heroku with git backend store",
  "repository": "https://github.com/std2main/bitwardenrs_heroku/tree/git_store",
  "website": "https://github.com/std2main/bitwardenrs_heroku/tree/git_store",
  "keywords": ["bitwarden", "git", "password", "otp"],
	"env": {
	  "DOMAIN": {
			"description": "Domain of this site, like https://bw.example.com",
			"value": "https://bw.example.com"
		},
		"ENABLE_ADMIN": {
			"description": "Enable /admin access or not",
      "value": "false"
		},
		"BACKUP_DB_REPO_URL": {
			"description": "git repo to store database",
      "value": "git://"
		},
		"BACKUP_FILE_REPO_URL": {
			"description": "git repo to store attachments",
      "value": "git://"
		},
		"BACKUP_GIT_SSH_KEY_B64": {
			"description": "Base64 content for the ssh key to access git repos",
      "value": ""
		},
    "POLLING_BACKUP": {
			"description": "Disable polling upload changes to repo when not equal true.",
      "value": "true"
    },
		"ENABLE_ADMIN": {
			"description": "Enable /admin access or not",
      "value": "false"
		},
    "GEN_ADMIN_TOKEN": {
      "description": "Autogenerated Admin token",
      "generator": "secret"
    }
	},
  "stack": "container"
}
